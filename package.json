{
  "name": "vue3-icon",
  "version": "1.0.2",
  "description": "Icon component for Vue 3",
  "main": "dist/vue3-icon.cjs.js",
  "module": "dist/vue3-icon.es.js",
  "scripts": {
    "build": "npx rimraf dist && bili --format cjs --format umd --format umd-min --format es --external vue --moduleName vue3-icon",
    "build:vite": "vite build -c vite-build.config.js",
    "test": "vue-cli-service test:unit",
    "demo": "vite serve",
    "demo:build": "vite build",
    "demo:deploy": "gh-pages -d demo/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigrileyuk/vue3-icon.git"
  },
  "keywords": [
    "Vue",
    "icon",
    "mdi",
    "pictogrammers"
  ],
  "author": "Pictogrammers, James Coyle, Craig Riley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craigrileyuk/vue3-icon/issues"
  },
  "homepage": "https://github.com/craigrileyuk/vue3-icon#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@mdi/js": "^5.9.55",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/test-utils": "^2.0.0-beta.13",
    "babel-plugin-prismjs": "^2.0.1",
    "bili": "^5.0.0",
    "browser-env": "^3.2.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.3.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1",
    "prismjs": "^1.22.0",
    "sass": "^1.30.0",
    "vite": "^2.3.7",
    "vue": "^3.0.4",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
